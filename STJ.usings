    <Using Include="System.Text.Json.Serialization.JsonSerializableAttribute" Alias="JSerializableAttribute" />
    <Using Include="System.Text.Json.Serialization.JsonSerializerContext" Alias="JSerCtx" />
    <Using Include="System.Text.Json.Serialization.JsonSourceGenerationMode" Alias="JSrcGenMode" />
    <Using Include="System.Text.Json.Serialization.JsonSourceGenerationOptionsAttribute" Alias="JSrcGenOptsAttribute" />
    <Using Include="System.Text.Json.Serialization.JsonStringEnumConverter" Alias="JStringEnumConverter" />
    <Using Include="System.Text.Json.Serialization" />
    <Using Include="System.Text.Json" />
    <Using Include="System.Text.RegularExpressions" />
